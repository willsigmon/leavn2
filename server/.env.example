# Copy to .env and fill the blanks

# API key for OpenAI features
OPENAI_API_KEY=

# API key for Anthropic features (optional)
ANTHROPIC_API_KEY=

# Connection string for the Postgres database
DATABASE_URL=

# Secret used to sign session cookies
SESSION_SECRET=

# Comma-separated list of domains allowed for Replit auth
REPLIT_DOMAINS=

# Domain to use when developing locally or on Replit
REPLIT_DEV_DOMAIN=

# OIDC issuer URL (defaults to https://replit.com/oidc)
ISSUER_URL=

# Repl identifier used for OIDC authentication
REPL_ID=

# Port for the server to listen on
PORT=5000